users = [
    {"name":"Taro","age":18},
    {"name":"Hanako","age":30},
    {"name":"Jiro","age":70},
]

def get_member_type(age):
   if age < 20:
         return "child"
   elif age < 65:
         return "adult"
   else:
       return "senior"

for user in users:
    member_type = get_member_type(user["age"])
    print(f'{user["name"]}: {member_type}')
      

